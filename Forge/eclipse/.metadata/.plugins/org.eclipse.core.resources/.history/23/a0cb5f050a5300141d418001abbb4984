package com.russn.pc.block;

import net.minecraft.block.Block;
import net.minecraft.block.material.Material;

import com.russn.pc.common.ModRegistry;

public class NetherGoldOre extends Block {

	public NetherGoldOre(Material mat) {
		super(mat);
		this.setCreativeTab(ModRegistry.TabOres);
		this.setHardness(2);
		this.setHarvestLevel("pickaxe", 3);
	}
}