package com.russn.pc.block;

import net.minecraft.block.Block;
import net.minecraft.block.material.Material;

import com.russn.pc.common.PotatoCraft;

public class AdamantinePotatoBlock extends Block {

	public AdamantinePotatoBlock(Material mat) {
		super(mat);
		this.setCreativeTab(PotatoCraft.tabPotatoBlock);
		this.setResistance(99999999999999999F); //dont even try.
		this.setHardness(50F);
		this.setHarvestLevel("pickaxe", 4);
	}
}